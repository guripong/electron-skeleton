{
  "name": "skeleton-electron",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "pack-installer": "electron-builder"
  },
 "build": {
    "productName": "exefilename",
    "appId": "com.electron.myapp",
    "directories": {
      "buildResources": "static",
      "output": "output/",
      "app": "."
    },
    "copyright": "COPYRIGHTS ~~",
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "icon/b.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "language": 1042,
      "shortcutName": "ghelp"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  }
}
